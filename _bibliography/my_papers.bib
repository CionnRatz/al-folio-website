	keywords = {cyber-physical systems;cyber-attacks;least squares approximations;networked control systems;observers;sensors;watermarking;SISO filter;routing attack;routing attack;cyber-physical systems;defense mechanisms;malicious cyber-attacks;multiplicative sensor watermarking scheme;networked control systems;observer-based detector design;physical sensor re-routing attack;recursive least squares parameter estimation algorithm;robust adaptive threshold;routing attack detection;routing attack isolation;single input single output filter;control systems;Detectors;Routing;Watermarking},
	month = {May},
