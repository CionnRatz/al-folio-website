@inproceedings{Gonzales-Silva_2022mu,
	abbr = {ACC22},
	author = {Gonzales-Silva, Jean and van der Hoek, Daan and Mulders, Sebastiaan P. and Ferrari, Riccardo M.G. and van Wingerden, Jan-Willem},
	booktitle = {American Control Conference},
	date-added = {2022-04-04 22:16:12 +0200},
	date-modified = {2022-11-01 14:17:11 +0100},
	keywords = {wind energy; wind farms; control systems; WATEREYE},
	month = {June},
	sort-word = {C045},
	title = {A Switching Thrust Tracking Controller for Load Constrained Wind Turbines},
	year = {2022},
	abstract = {Wind turbines are prone to structural degradation, particularly in offshore locations. Based on the structural health condition of the tower, power de-rating strategies can be used to reduce structural loads at the cost of power losses. This paper introduces a novel closed-loop switching control architecture to constrain the thrust in individual turbines. By taking inspiration from developments in the field of reference governors, an existing demanded power tracking controller is extended by a thrust tracking controller. The latter is activated only when a user-defined constraint on fore-aft thrust force is exceeded, which can be set based on the actual damage status of the turbine. Having a down-regulation with monotonic aerodynamic load response, a simple linear thrust tracking controller is proposed. Such a scheme can reduce aerodynamic loads while incurring acceptable losses on power production which, in a wind farm setting, can be compensated for by other turbines. Large eddy simulations demonstrate the performance of the proposed scheme on satisfying thrust constraints.},
	bdsk-file-1 = {}}

@inproceedings{Pamososuryo_2022pz,
	abbr = {ACC22},
	author = {Pamososuryo, Atindriyo Kusumo and Mulders, Sebastiaan P. and Ferrari, Riccardo M.G. and van Wingerden, Jan-Willem},
	booktitle = {American Control Conference},
	date-added = {2022-04-04 22:12:21 +0200},
	date-modified = {2022-11-01 14:17:04 +0100},
	keywords = {wind energy; estimation; load reduction; EDOWE},
	month = {June},
	sort-word = {C044},
	title = {Periodic Load Estimation of a Wind Turbine Tower using a Model Demodulation Transformation},
	year = {2022},
	abstract = {Individual Pitch Control (IPC) is an effective control strategy to mitigate the blade loads on large-scale wind turbines. Since IPC usually requires high pitch actuation, the safety constraints of the pitch actuator should be taken into account when designing the controller. This paper introduces a constrained Subspace Predictive Repetitive Control (SPRC) approach, which considers the limitation of blade pitch angle and pitch rate. To fulfill this goal, a model predictive control scheme is implemented in the fully data-driven SPRC approach to incorporate the physical limitations of the pitch actuator in the control problem formulation. An optimal control law subjected to constraints is then formulated so that future constraint violations are anticipated and prevented. Case studies show that the developed constrained SPRC reduces the pitch activities necessary to mitigate the blade loads when experiencing wind turbulence and abrupt wind gusts. More importantly, the approach allows the wind farm operator to design conservative bounds for the pitch actuator constraints that satisfies safety limitations, design specifications and physical restrictions. This will help to alleviate the cyclic fatigue loads on the actuators, increase the structural reliability and extend the lifespan of the pitch control system.},
	bdsk-file-1 = {}}

