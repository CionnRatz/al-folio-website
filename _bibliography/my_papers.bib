@article{Liu_2022sh,
	abbr = {IEEE_LCSS},
	arxiv = {2110.00639},
	author = {Liu, Yichao and Pamososuryo, Atindriyo Kusumo and Mulders, Sebastiaan P. and Ferrari, Riccardo M.G. and van Wingerden, Jan-Willem},
	date-added = {2022-01-13 10:04:17 +0100},
