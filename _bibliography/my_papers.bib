	author = {Baioumy, Mohamed and Pezzato, Corrado and Ferrari, Riccardo M.G. and Hawes, Nick},
	booktitle = {International Conference on Intelligent Robots and Systems},
	date-added = {2022-09-03 17:00:29 +0200},
